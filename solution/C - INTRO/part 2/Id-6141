#include<stdio.h>

int main()
{
    int X, Y;
    char ch;
    scanf("%d %d\n%c", &X, &Y, &ch);
    switch(ch){
        case '+':
            printf("%d", X+Y);
            break;
        case '-':
            printf("%d", X - Y);
            break;
        case '*':
            printf("%d", X * Y);
            break;
        case '/':
            printf("%d", X / Y);
            break;
        case '%':
            printf("%d", X % Y);
            break;
        default:
        printf("INVALIDOPERATION");
    }
    return 0;
}