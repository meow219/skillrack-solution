#include <stdio.h>

int main() {
    int N, num, sum = 0;
    
    scanf("%d", &N);
    
    for (int i = 0; i < N; ++i) {
        scanf("%d", &num);
        if (num % 10 == 3 || num % 10 == 6) {
            sum += num;
        }
    }
    
    if (sum == 0) {
        printf("-1\n");
    } else {
        printf("%d\n", sum);
    }

    return 0;
}
