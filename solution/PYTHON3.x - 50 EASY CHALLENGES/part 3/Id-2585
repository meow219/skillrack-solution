S = input()

expanded_string = ""
current_number = ""

for char in S:
    if char.isdigit():
        current_number += char
    else:
        if current_number:
            expanded_string += int(current_number) * char
            current_number = ""
        else:
            expanded_string += char

# Check if there is a pending number after the loop
if current_number:
    expanded_string += int(current_number) * char

print(expanded_string)
