def count_common_characters(s1, s2):
    set_s1 = set(s1)
    set_s2 = set(s2)
    
    common_characters = set_s1.intersection(set_s2)
    
    return len(common_characters)

# Input
s1 = input().strip()
s2 = input().strip()

# Output
result = count_common_characters(s1, s2)
print(result)
