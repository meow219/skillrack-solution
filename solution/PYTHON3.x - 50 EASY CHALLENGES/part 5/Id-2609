def transform_string(s):
    words = s.split()

    result_words = []
    for word in words:
        transformed_word = ''
        for i in range(len(word)):
            if i % 2 == 0:
                transformed_word += word[i].upper()
            else:
                transformed_word += word[i].lower()
        result_words.append(transformed_word)

    result_string = ' '.join(result_words)
    print(result_string)

# Input
S = input().strip()

# Output
transform_string(S)
