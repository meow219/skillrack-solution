def find_common_part(s1, s2):
   
    common_part = ""
    min_length = min(len(s1), len(s2))
    
    for i in range(min_length):
        if s1[-(i+1):] == s2[:i+1]:
            common_part = s1[-(i+1):]
    
    return common_part


s1 = input().strip()
s2 = input().strip()


common_part = find_common_part(s1, s2)
print(common_part)
