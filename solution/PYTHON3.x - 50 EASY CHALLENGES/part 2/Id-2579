def find_missing_alphabet(s):
    n = len(s)
    start = 0
    end = n - 1
    
    while start <= end:
        if s[start] == s[end]:
            start += 1
            end -= 1
        else:
            if s[start + 1] == s[end]:
                return s[start]
            elif s[start] == s[end - 1]:
                return s[end]
    
    return None

s = input().strip()
missing_alphabet = find_missing_alphabet(s)
print(missing_alphabet)
