#include<stdio.h>

int main()
{
    int N;
    scanf("%d", &N);
    int arr[N], index, cmpIndex, pairsCount = 0;
    for(index = 0; index < N; index++)
    {
        scanf("%d", &arr[index]);
    }
    for(index = 0; index < N-1; index++)
    {
        for(cmpIndex = index + 1; cmpIndex < N; cmpIndex++)
        {
            if(arr[index] > arr[cmpIndex])
            {
                pairsCount++;
            }
        }
    }
    printf("%d", pairsCount);
    return 0;
}